        <item name="strokeWidth">0dp</item>
    </style>
    <style name="TextAppearance.PriceWatch.DisplayLarge" parent="TextAppearance.Material3.BodyLarge">
        <item name="fontFamily"></item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">20sp</item>
        <item name="android:letterSpacing">0</item>
    </style>
    <style name="TextAppearance.PriceWatch.DisplayMedium" parent="TextAppearance.Material3.BodyMedium">
